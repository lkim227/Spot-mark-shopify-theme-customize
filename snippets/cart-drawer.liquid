						<a href="{{ routes.cart_url }}" class="cart__link button button--secondary button--full-width">
							<span>{{ 'general.cart.view' | t: count: cart.item_count }}</span>

						</a>
					
					</div>
				</div>
			</div>

		{%- endif -%}
		</div>
	</div>
</cart-drawer>

