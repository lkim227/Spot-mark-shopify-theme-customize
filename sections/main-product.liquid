<script defer>
	
	let show_more = document.querySelectorAll(".show_more_btn");
	show_more.forEach((item) => {
		item.addEventListener("click", () => {
			let text_span = item.querySelector("span:last-child");
			let description_box = item.parentElement;
			if(text_span.innerText == "Show more"){
				description_box.style.maxHeight = description_box.scrollHeight + "px";
				text_span.innerText = "Show less";
			}
			else{
				text_span.innerText = "Show more";
				description_box.style.maxHeight =  "170px";
			}
			
		});

	});
</script>

